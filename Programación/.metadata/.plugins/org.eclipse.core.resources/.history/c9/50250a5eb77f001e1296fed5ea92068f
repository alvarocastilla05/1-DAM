package ejercicio05;

public class Frases {
	
	private String texto;
	private int num;
	
	public Frases(String texto, int num) {
		super();
		this.texto = texto;
		this.num = num;
	}

	
	public String getTexto() {
		return texto;
	}

	public void setTexto(String texto) {
		this.texto = texto;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}


	@Override
	public String toString() {
		return "Frases: "+num +"."+ texto+" ]";
	}
	
	
	
	
	
	

}
